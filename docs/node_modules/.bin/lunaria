#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/dist/cli/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/dist/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/dist/cli/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/dist/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/core/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules/@lunariajs/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/@lunariajs+core@0.0.31/node_modules:/Users/kingwww/VSCode/arvis/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@lunariajs/core/dist/cli/index.mjs" "$@"
else
  exec node  "$basedir/../@lunariajs/core/dist/cli/index.mjs" "$@"
fi
